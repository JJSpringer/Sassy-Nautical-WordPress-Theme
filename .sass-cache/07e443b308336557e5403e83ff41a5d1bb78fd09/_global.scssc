// =Sass Functions
$darkblue: #0B2E46;
$lightblue: #6291b6;
$coral: #f79366;
$beige: #fffdf9;
$gray: #222222;

$headerfont: 'Roboto Slab', Constantia, "Lucida Bright", Lucidabright, "Lucida Serif", Lucida, "DejaVu Serif", "Bitstream Vera Serif", "Liberation Serif", Georgia, serif;
$bodyfont: 'Open Sans', Frutiger, "Frutiger Linotype", Univers, Calibri, "Gill Sans", "Gill Sans MT", "Myriad Pro", Myriad, Tahoma, Geneva, "Helvetica Neue", Helvetica, Arial, sans-serif;

@mixin metafooter{
	font-family: $headerfont;
	font-weight: 300;
	color: #6291B6;
	font-size: 1em;
}

// =Rest of Global
body,
button,
input,
select,
textarea {
	color: $gray;
	font-family: $bodyfont;
	font-weight: 300;
	font-size: 16px;
	font-size: 1.6rem;
	line-height: 1.5;
}

body {
	background-color: $darkblue;
}

/* Headings */
h1,h2,h3,h4,h5,h6 {
	clear: both;
	color: lighten($darkblue, 10%);
}
h1 {
	font-size: 2.5em;
}
h2 {
	font-size: 1.8em;
	font-weight: 600;
	text-transform: uppercase;
}
h3{
	font-size: 1.7em;
}
h4 {
	font-size: 1.5em;
}
h5 {
	font-size: 1em;
}
h6 {
	font-size: .75em;
}
hr {
	background-color: #ccc;
	border: 0;
	height: 1px;
	margin-bottom: 1.5em;
}

/* Text elements */
p {
	margin-bottom: 1.5em;
}
ul, ol {
	margin: 0 0 1.5em 3em;
}
ul {
	list-style: disc;
}
ol {
	list-style: decimal;
}
li > ul,
li > ol {
	margin-bottom: 0;
	margin-left: 1.5em;
}
dt {
	font-weight: bold;
	color:	$lightblue;
}
dd {
	margin: 0 1.5em 1.5em;
}
b, strong {
	font-weight: bold;
}
dfn, cite, em, i {
	font-style: italic;
}
blockquote {
	margin: 0 1.5em;
}
address {
	margin: 0 0 1.5em;
	color:	$coral;
	font-weight: 600;
}
pre {
	background: #eee;
	font-family: "Courier 10 Pitch", Courier, monospace;
	font-size: 15px;
	font-size: 1.5rem;
	line-height: 1.6;
	margin-bottom: 1.6em;
	padding: 1.6em;
	overflow: auto;
	max-width: 100%;
}
code, kbd, tt, var {
	font: 15px Monaco, Consolas, "Andale Mono", "DejaVu Sans Mono", monospace;
}
abbr, acronym {
	border-bottom: 1px dotted #666;
	cursor: help;
}
mark, ins {
	background: #fff9c0;
	text-decoration: none;
}
sup,
sub {
	font-size: 75%;
	height: 0;
	line-height: 0;
	position: relative;
	vertical-align: baseline;
}
sup {
	bottom: 1ex;
}
sub {
	top: .5ex;
}
small {
	font-size: 75%;
}
big {
	font-size: 125%;
}
figure {
	margin: 0;
}
table {
	margin: 0 0 1.5em;
	width: 100%;
}
th {
	font-weight: bold;
	color:	$lightblue;
}
img {
	height: auto; /* Make sure images are scaled correctly. */
	max-width: 100%; /* Adhere to container width. */
	-ms-interpolation-mode: bicubic;
}
button,
input,
select,
textarea {
	font-size: 100%; /* Corrects font size not being inherited in all browsers */
	margin: 0; /* Addresses margins set differently in IE6/7, F3/4, S5, Chrome */
	vertical-align: baseline; /* Improves appearance and consistency in all browsers */
	*vertical-align: middle; /* Improves appearance and consistency in all browsers */
}
button,
input {
	line-height: normal; /* Addresses FF3/4 setting line-height using !important in the UA stylesheet */
}
button,
html input[type="button"],
input[type="reset"],
input[type="submit"] {
	@include sassy-button(flat, 0px, 1em, $coral, $coral, white, false, true);
}
button:hover,
html input[type="button"]:hover,
input[type="reset"]:hover,
input[type="submit"]:hover {
	@include sassy-button(flat, 0px, 1em, $coral, $coral, white, false, true);
	color: white;
	text-decoration: none;
	}
button:focus,
html input[type="button"]:focus,
input[type="reset"]:focus,
input[type="submit"]:focus,
button:active,
html input[type="button"]:active,
input[type="reset"]:active,
input[type="submit"]:active {
	@include sassy-button(flat, 0px, 1em, $coral, $coral, white, false, true);
	color: white;
	text-decoration: none;
}
input[type="checkbox"],
input[type="radio"] {
	box-sizing: border-box; /* Addresses box sizing set to content-box in IE8/9 */
	padding: 0; /* Addresses excess padding in IE8/9 */
}
input[type="search"] {
	-webkit-appearance: textfield; /* Addresses appearance set to searchfield in S5, Chrome */
	-webkit-box-sizing: content-box; /* Addresses box sizing set to border-box in S5, Chrome (include -moz to future-proof) */
	-moz-box-sizing:    content-box;
	box-sizing:         content-box;
}
input[type="search"]::-webkit-search-decoration { /* Corrects inner padding displayed oddly in S5, Chrome on OSX */
	-webkit-appearance: none;
}
button::-moz-focus-inner,
input::-moz-focus-inner { /* Corrects inner padding and border displayed oddly in FF3/4 www.sitepen.com/blog/2008/05/14/the-devils-in-the-details-fixing-dojos-toolbar-buttons/ */
	border: 0;
	padding: 0;
}
input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"],
textarea {
	color: #666;
	border: 1px solid #ccc;
	border-radius: 3px;
}
input[type="text"]:focus,
input[type="email"]:focus,
input[type="password"]:focus,
input[type="search"]:focus,
textarea:focus {
	color: #111;
}
input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"] {
	padding: 3px;
}
textarea {
	overflow: auto; /* Removes default vertical scrollbar in IE6/7/8/9 */
	padding-left: 3px;
	vertical-align: top; /* Improves readability and alignment in all browsers */
	width: 98%;
}

/* Links */
a {
	color: $lightblue;
}
a:visited {
	color: $darkblue;
}
a:hover,
a:focus,
a:active {
	color: darken($lightblue, 15%);
}


/* Alignment */
.alignleft {
	display: inline;
	float: left;
	margin-right: 1.5em;
}
.alignright {
	display: inline;
	float: right;
	margin-left: 1.5em;
}
.aligncenter {
	clear: both;
	display: block;
	margin: 0 auto;
}

/* Text meant only for screen readers */
.screen-reader-text {
	clip: rect(1px, 1px, 1px, 1px);
	position: absolute !important;
}

.screen-reader-text:hover,
.screen-reader-text:active,
.screen-reader-text:focus {
	background-color: #f1f1f1;
	border-radius: 3px;
	box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.6);
	clip: auto !important;
	color: $lightblue;
	display: block;
	font-size: 14px;
	font-weight: bold;
	height: auto;
	left: 5px;
	line-height: normal;
	padding: 15px 23px 14px;
	text-decoration: none;
	top: 5px;
	width: auto;
	z-index: 100000; /* Above WP toolbar */
}

/* Clearing */
.clear:before,
.clear:after,
[class*="content"]:before,
[class*="content"]:after,
[class*="site"]:before,
[class*="site"]:after {
	content: '';
	display: table;
}

.clear:after,
[class*="content"]:after,
[class*="site"]:after {
	clear: both;
}

/* Nautical Theme Custom CSS */
#page {
	background-color: $beige;
	margin: 2em auto;
	width: 70%; 
	max-width: 984px;
	border-width: 10px;
	border-color: $coral;
	border-style: solid;
	padding: 3%; 
}
.site-header {
	position: relative;
	color: $beige;
	display: block;
	margin: 0 auto;
	max-width: 984px;
	text-decoration: none;
	width: 100%;

}

/* Header */
@import "icons/*.png";

.anchor{
	background: url(images/img_anchor.png) no-repeat;
	background-size: 100%;
	background-position: center;
	width: 10%;
	height: 100px;
	max-width: 100px;
	min-width: 50px;
	margin-top: .5em;
	float:left;
	display: inline-block;
	@media screen and (max-width: 900px) {
	float: none;
	width: 100px;
	margin: 0 auto;
	display: block;
	}
}
.site-branding{
	margin-left: 5%;
	vertical-align: middle;
	margin: 0 auto;
	max-width: 984px;
	padding: 0 20px;
	display: inline-block;
	margin-bottom: 30px;
	@media screen and (max-width: 900px) {
	float:none;
	display: block;
	text-align: center;
	}
}
.site-title {
	font-size: 3em; 
	font-family: $headerfont;
	font-weight: 100;
	text-decoration: none;
	display: block;
	& a {text-decoration: none;}
		&:hover,:active,:visted {text-decoration: none;}
}

.site-description {
	color: $lightblue;
	text-transform: uppercase;
	font-size: 1em;
	font-family: $headerfont;
	font-weight: 300;
}

#current_page_item {
	text-decoration: underline;
}

/* Posts */
.entry-title {
	font-family: $headerfont;
	font-weight: 300;
	color: $coral;
	font-size: 1.5em;
	margin-top: 2em;
    position:relative;
    	border-bottom: dashed 2px $coral;
    & a{
	color: $coral;
	text-decoration: none;
    }
    	&:hover{
	    	text-decoration: underline;
    	}
    &:before {
	content: "";
	@include icons-sprite(star);
	height: icons-sprite-height(star);
	width: icons-sprite-width(star);
	display: inline-block;
	margin: 0 5px;
    }
}

/* Meta */
.entry-meta {
	margin-left: 1.5em;
	padding-bottom: 1.5em;
	border-bottom: 1px solid;
	border-color: darken($beige, 10%);
	@media screen and (max-width: 800px) {
	margin-left: 0;
	}

	}

.sep  {
	margin-left: 6px;
	color: $lightblue;
}

.entry-date, .author, .cat-links, .tags-links, .post-edit-link, .comments-link, .sep  {
	@include metafooter;
}

.entry-date:before {
	content: "";
	@include icons-sprite(clock);
	height: icons-sprite-height(clock);
	width: icons-sprite-width(clock);
	display: inline-block;
	margin: 0 5px;
}

.author:before {
	content: "";
	@include icons-sprite(person);
	height: icons-sprite-height(person);
	width: icons-sprite-width(person);
	display: inline-block;
	margin: 0 5px;
}

.cat-links:before {
	content: "";
	@include icons-sprite(location);
	height: icons-sprite-height(location);
	width: icons-sprite-width(location);
	display: inline-block;
	margin: 0 5px;
}

.post-edit-link:before {
	content: "";
	@include icons-sprite(gear);
	height: icons-sprite-height(gear);
	width: icons-sprite-width(gear);
	display: inline-block;
	margin: 0 5px;
}

.comment-edit-link:before {
	content: "";
	@include icons-sprite(gear);
	height: icons-sprite-height(gear);
	width: icons-sprite-width(gear);
	display: inline-block;
	margin: 0 5px;
}
.comments-link:before {
	content: "";
	@include icons-sprite(bubble);
	height: icons-sprite-height(bubble);
	width: icons-sprite-width(bubble);
	display: inline-block;
	margin: 0 5px;
}

.tags-links:before {
	content: "";
	@include icons-sprite(tag);
	height: icons-sprite-height(tag);
	width: icons-sprite-width(tag);
	display: inline-block;
	margin: 0 5px;
}

.more-link {
	float: left;
	@include sassy-button(flat, 0px, 1em, $lightblue, $lightblue, white, false, true);
	margin: 10px 0 25px 0;
	text-transform: uppercase;
	&:hover{
		color: white;
		text-decoration: none;
	}
	@media screen and (max-width: 850px) {
	@include sassy-button(flat, 0px, .8em, $lightblue, $lightblue, white, false, true);
	margin: 5px 0;
	}
}

/* Featured Images */
.entry-thumbnail {
	max-width: 984;
	margin-left: 1.5em;
	margin-top: .5em;
	border: 3px solid $lightblue;
	@media screen and (max-width: 800px) {
	margin-left: 0;
	}
}
.entry-thumbnail img {
	display: block;
	margin: 0 auto;
	width: 100%;	
}
	
.site-info {
	width: 100%;
	text-align: center;
}

/* Footer */
.site-footer{
	clear:both;
	width:100%;
	border-top: 1px solid $lightblue;
	padding-top: 20px;
}
